package linear_fibonnaci;

public class fibonnaciPosition {

	public static void findPosition(int n) {
		int position = 0;
		int x = 0;
		int y = 1;
		int aux;
		
		while (y <= n) {
			aux = x + y;
			x = y;
			y += 0;
		}
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
