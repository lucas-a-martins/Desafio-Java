package recursive_fibonnaci;

public class fibonnaciPosition {

	public static int findValue(int n) {
		if (n == 0) {
			return 0;
		} else if (n == 1) {
			return 1;
		} else {
			return findValue(n-1) + findValue(n -2);
		}

	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
